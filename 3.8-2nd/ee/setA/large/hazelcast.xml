<hazelcast xsi:schemaLocation="http://www.hazelcast.com/schema/config
                               http://www.hazelcast.com/schema/config/hazelcast-config-3.8.xsd"
           xmlns="http://www.hazelcast.com/schema/config"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <group>
        <name>workers</name>
    </group>

    <network>
        <port port-count="200" auto-increment="true">5701</port>
        <join>
            <multicast enabled="false"/>
            <tcp-ip enabled="true">
                <!--MEMBERS-->
            </tcp-ip>
        </join>
    </network>

    <properties>
        <property name="hazelcast.phone.home.enabled">false</property>
    </properties>

    <!--LICENSE-KEY-->

    <!--MANAGEMENT_CENTER_CONFIG-->

    <serialization>
        <portable-version>1</portable-version>

        <data-serializable-factories>
            <data-serializable-factory factory-id="2000">
                com.hazelcast.simulator.tests.synthetic.SyntheticSerializableFactory
            </data-serializable-factory>
            <data-serializable-factory factory-id="3000">
                com.hazelcast.simulator.tests.syntheticmap.SyntheticMapSerializableFactory
            </data-serializable-factory>
            <data-serializable-factory factory-id="4000">
                com.hazelcast.simulator.tests.map.domain.IdentifiedDataSerializableObjectFactory
            </data-serializable-factory>
        </data-serializable-factories>

        <portable-factories>
            <portable-factory factory-id="10000001">
                com.hazelcast.simulator.tests.map.domain.PortableObjectFactory
            </portable-factory>
            <portable-factory factory-id="10000002">
                com.hazelcast.simulator.tests.map.helpers.ComplexDomainObjectPortableFactory
            </portable-factory>
            <portable-factory factory-id="5000">
                com.hazelcast.simulator.tests.map.ExtractorMapTest$SillySequencePortableFactory
            </portable-factory>
            <portable-factory factory-id="5001">
                com.hazelcast.simulator.tests.map.MultiValueMapTest$SillySequencePortableFactory
            </portable-factory>
        </portable-factories>
    </serialization>

    <native-memory allocator-type="POOLED" enabled="true">
        <size unit="GIGABYTES" value="4" />
        <metadata-space-percentage>20</metadata-space-percentage>
    </native-memory>

    <map name="*">
        <in-memory-format>NATIVE</in-memory-format>
        <eviction-policy>LFU</eviction-policy>
        <eviction-percentage>1</eviction-percentage>
        <max-size policy="USED_NATIVE_MEMORY_PERCENTAGE">99</max-size>
    </map>

    <cache name="*">
        <backup-count>1</backup-count>
        <async-backup-count>0</async-backup-count>
        <in-memory-format>NATIVE</in-memory-format>
        <eviction size="99" max-size-policy="USED_NATIVE_MEMORY_PERCENTAGE" eviction-policy="LRU" />
    </cache>

    <map name="MapStore*">
        <in-memory-format>NATIVE</in-memory-format>
        <eviction-policy>LFU</eviction-policy>
        <eviction-percentage>1</eviction-percentage>
        <max-size policy="USED_NATIVE_MEMORY_PERCENTAGE">99</max-size>
        <map-store enabled="true">
            <class-name>com.hazelcast.simulator.tests.map.helpers.MapStoreWithCounter</class-name>
            <write-delay-seconds>5</write-delay-seconds>
        </map-store>
    </map>

    <map name="MapEvictAndStore*">
        <in-memory-format>NATIVE</in-memory-format>
        <eviction-policy>LFU</eviction-policy>
        <eviction-percentage>1</eviction-percentage>
        <max-size policy="PER_NODE">5000</max-size>
        <map-store enabled="true">
            <class-name>com.hazelcast.simulator.tests.map.helpers.MapStoreWithCounterPerKey</class-name>
            <write-delay-seconds>5</write-delay-seconds>
            <write-coalescing>false</write-coalescing>
        </map-store>
        <time-to-live-seconds>10</time-to-live-seconds>
    </map>

    <map name="MapMaxSize*">
        <in-memory-format>NATIVE</in-memory-format>
        <eviction-policy>LFU</eviction-policy>
        <eviction-percentage>1</eviction-percentage>
        <max-size policy="PER_NODE">1000</max-size>
    </map>

    <map name="ExtractorMapTest*">
        <in-memory-format>NATIVE</in-memory-format>
        <eviction-policy>LFU</eviction-policy>
        <eviction-percentage>1</eviction-percentage>
        <max-size policy="USED_NATIVE_MEMORY_PERCENTAGE">99</max-size>
        <attributes>
            <attribute extractor="com.hazelcast.simulator.tests.map.ExtractorMapTest$PayloadExtractor">
                payloadFromExtractor
            </attribute>
        </attributes>
    </map>

    <map name="PortableExtractorMapTest*">
        <in-memory-format>NATIVE</in-memory-format>
        <eviction-policy>LFU</eviction-policy>
        <eviction-percentage>1</eviction-percentage>
        <max-size policy="USED_NATIVE_MEMORY_PERCENTAGE">99</max-size>
        <attributes>
            <attribute extractor="com.hazelcast.simulator.tests.map.ExtractorMapTest$PayloadPortableExtractor">
                payloadFromExtractor
            </attribute>
        </attributes>
    </map>

    <map name="quorum-map">
        <in-memory-format>NATIVE</in-memory-format>
        <eviction-policy>LFU</eviction-policy>
        <eviction-percentage>1</eviction-percentage>
        <max-size policy="USED_NATIVE_MEMORY_PERCENTAGE">99</max-size>
        <backup-count>1</backup-count>
        <quorum-ref>map-quorum-ref</quorum-ref>
    </map>

    <cache name="quorum-cache">
        <backup-count>1</backup-count>
        <in-memory-format>NATIVE</in-memory-format>
        <eviction size="99" max-size-policy="USED_NATIVE_MEMORY_PERCENTAGE" eviction-policy="LRU" />
        <quorum-ref>cache-quorum-ref</quorum-ref>
    </cache>

    <quorum name="map-quorum-ref" enabled="true">
        <quorum-size>3</quorum-size>
    </quorum>

    <quorum name="cache-quorum-ref" enabled="true">
        <quorum-size>3</quorum-size>
    </quorum>

    <cache name="maxCacheSmall*">
        <in-memory-format>NATIVE</in-memory-format>
        <eviction max-size-policy="ENTRY_COUNT" size="271" eviction-policy="LFU"/>
    </cache>

    <cache name="maxCacheMedium*">
        <in-memory-format>NATIVE</in-memory-format>
        <eviction max-size-policy="ENTRY_COUNT" size="1000" eviction-policy="LFU"/>
    </cache>

    <cache name="maxCacheLarge*">
        <in-memory-format>NATIVE</in-memory-format>
        <eviction max-size-policy="ENTRY_COUNT" size="10000" eviction-policy="LFU"/>
    </cache>
</hazelcast>
